  "repository": {
    "type": "git",
    "url": "https://github.com/spoonx/aurelia-authentication"
  },
  "scripts": {
    "test": "gulp test",
    "build": "gulp build",
    "preversion": "npm test",
    "version": "npm run build && gulp prepare-package && git add -A dist doc/CHANGELOG.md bower.json",
    "postpublish": "git push && git push --tags"
