      "dist": "dist/amd"
    },
    "dependencies": {
      "aurelia-api": "^3.0.0",
      "aurelia-dependency-injection": "^1.0.0",
      "aurelia-event-aggregator": "^1.0.0",
      "aurelia-fetch-client": "^1.0.0",
      "aurelia-logging": "^1.0.0",
      "aurelia-metadata": "^1.0.0",
      "aurelia-pal": "^1.0.0",
      "aurelia-path": "^1.0.0",
      "aurelia-router": "^1.0.0",
      "aurelia-templating-resources": "^1.0.0",
      "extend": "^3.0.0",
      "jwt-decode": "^2.0.0"
    },
    "peerDependencies": {
      "aurelia-api": "^3.0.0",
      "aurelia-dependency-injection": "^1.0.0",
      "aurelia-event-aggregator": "^1.0.0",
      "aurelia-fetch-client": "^1.0.0",
      "aurelia-logging": "^1.0.0",
      "aurelia-metadata": "^1.0.0",
      "aurelia-pal": "^1.0.0",
      "aurelia-path": "^1.0.0",
      "aurelia-router": "^1.0.0",
      "aurelia-templating-resources": "^1.0.0",
      "extend": "^3.0.0",
      "jwt-decode": "^2.0.0"
    },
    "devDependencies": {
      "aurelia-binding": "^1.0.0",
      "aurelia-bootstrapper": "^1.0.0",
      "aurelia-pal-browser": "^1.0.0",
      "aurelia-polyfills": "^1.0.0",
      "fetch": "github:github/fetch@^1.0.0"
    }
  },
  "dependencies": {
    "aurelia-api": "^3.0.0",
    "aurelia-dependency-injection": "^1.0.0",
    "aurelia-event-aggregator": "^1.0.0",
    "aurelia-fetch-client": "^1.0.0",
    "aurelia-logging": "^1.0.0",
    "aurelia-metadata": "^1.0.0",
    "aurelia-pal": "^1.0.0",
    "aurelia-path": "^1.0.0",
    "aurelia-router": "^1.0.0",
    "aurelia-templating-resources": "^1.0.0",
    "extend": "^3.0.0",
    "jwt-decode": "^2.0.0"
  },
  "devDependencies": {
    "gulp-json-format": "^1.0.0",
    "spoonx-tools": "^0.3.0"
  },
  "aurelia": {
    "build": {
      "resources": [
        "aurelia-authentication/authFilterValueConverter",
        "aurelia-authentication/authenticatedFilterValueConverter",
        "aurelia-authentication/authenticatedValueConverter"
      ]
    }
  }
}